extends layoutblock content    script(type="text/javascript", src="//code.jquery.com/ui/1.10.3/jquery-ui.js")    script(type="text/javascript", src="/socket.io/socket.io.js")    script(type="text/javascript", src="/ace/ace.js")    script(type="text/javascript", src="/javascripts/coding.js")    input(type="button", value="Index" onclick="window.location.href='/'")    if !user.guest        input(type="button", value="Profile" onclick="window.location.href='/profile'")    input(type="button", value="Study" onclick="window.location.href='/study'")    input(type="button", value="Coding" onclick="window.location.href='/coding'")    input(type="button", value="Sumorobot" onclick="window.location.href='/sumorobot'")    input(type="button", value="Exclusive" onclick="inviteToECode()")    h3 Online users:    div#online-users        p Loading users ...    div.clear    div#silverWraper        div#task        div#code-win WIN!        div#code-points        div#code-message        div#code-wrapper            span Test Coding Abilities            div#code    div#eCode(style="display:none;")        div#player1(style="float:left;width:45%;")        div#player2(style="margin-left:10px;width:45%;")        div.clear    div#hiddenDialogs(style="display:none;")        div#inviteExclusive            div#oUser        div#incomingInvitation